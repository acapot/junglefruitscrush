	package  {		import flash.display.MovieClip;	import flash.events.MouseEvent;		public class BtnFriend extends MovieClip {						public var frameNum1:int;		public var frameNum2:int;				public function BtnFriend(frn1:int, frn2:int) {			// constructor code			//this.y=30;			frameNum1 = frn1;			frameNum2 = frn2;			goToFrame();					}				public function goToFrame()		{						gotoAndStop(frameNum1);			addEventListener(MouseEvent.MOUSE_OVER, changeFrameOver);			addEventListener(MouseEvent.MOUSE_OUT, changeFrameOut);			//gotoAndPlay("accept");					}				public function changeFrameOver(event:MouseEvent)		{						gotoAndStop(frameNum2);					}				public function changeFrameOut(event:MouseEvent)		{			gotoAndStop(frameNum1);					}				public function removeEvents()		 {						if (parent.contains(this))			{				removeEventListener(MouseEvent.MOUSE_OVER, changeFrameOver);				removeEventListener(MouseEvent.MOUSE_OUT, changeFrameOut);				parent.removeChild(this);				trace("lo elimina");							}					 }							}	}